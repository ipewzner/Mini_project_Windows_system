                </TextBlock>
                <TextBlock HorizontalAlignment="Stretch" Height="20"    VerticalAlignment="Center"  >
              <CheckBox x:Name="NaerPublicTrensportion" Content="NaerPublicTrensportion" HorizontalAlignment="Stretch"  VerticalAlignment="Stretch"/>
                </TextBlock>
            </StackPanel>
        </ScrollViewer>
    </Grid>
</UserControl>
